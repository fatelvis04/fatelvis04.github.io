<nav id="footer-nav" tabindex="-1" aria-label="footer">
<ul role="list" id="footer-nav-list">
{% set navPages = collections.all | eleventyNavigation %}
{%- for entry in navPages %}
<li class="footer-nav-list-item"{% if entry.url == page.url %} aria-current="page"{% endif %}>
{% if entry.url != page.url %}<a href="{{ entry.url }}">{% endif %}{{ entry.title }}{% if entry.url != page.url %}</a>{% endif %}
{% if (loop.index <= navPages | length - 1) %}<span class="footer-nav-list-item-separator" aria-hidden="true"> | </span>{% endif %}
</li>
{%- endfor %}
</ul>
</nav>        